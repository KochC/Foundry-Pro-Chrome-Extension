{
    "manifest_version": 3,
    "name": "Foundry Pro Chrome Extension",
    "description": "This tool adds new menu items to the Palantir Foundry UI for developers.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "matches": ["*://*/*"],
            "resources": ["src/*", "libs/*"]
        }
    ]
}
