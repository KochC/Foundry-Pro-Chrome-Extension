{
    "manifest_version": 3,
    "name": "Foundry-cli",
    "description": "This is an example of a Chrome extension.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "matches": ["*://*/*"],
            "resources": ["src/*", "libs/*"]
        }
    ]
}
